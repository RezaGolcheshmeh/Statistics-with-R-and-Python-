import pandas as pd
# Create DataFrame
df = pd.DataFrame({
'Height': [174, 180, 165, 190],
'Weight': [80, 85, 70, 95],
'Gender': ['Male', 'Male', 'Female', 'Male']
})
# Group by Gender and calculate percentiles
percentiles = df.groupby('Gender')[['Height', 'Weight']].quantile([0.25, 0.5, 0.75])
print(percentiles)
