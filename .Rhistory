tapply(data$Anxiety, data$Gender, median, na.rm = TRUE)
tapply(data$Depression, data$Gender, median, na.rm = TRUE)
aggregate(cbind(Anxiety, Depression) ~ Gender, data = data, median,na.rm = TRUE)
