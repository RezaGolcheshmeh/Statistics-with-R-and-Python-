import pandas as pd
import numpy as np
# ساخت دیتافریم
data = {
"Age": [24, 30, 22, 24, 30, 24],
"Score": [85, np.nan, 78, 92, np.nan, 95],
"Gender": ["F", "M", "M", "F", "M", "F"]
}
df = pd.DataFrame(data)
print(df)
medians = df.median(numeric_only=True, skipna=True)
print(medians)
group_medians = df.groupby("Gender").median(numeric_only=True, skipna=True)
print(group_medians)
group_medians = df.groupby("Gender").median(numeric_only=True)
